{
    "name": "errohitsinghal/yii2-ajaxcrud-bs5-advanced",
    "description": "Gii CRUD template with password validation modal for Single Page Ajax Administration for yii2 with Bootstrap 5 support",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","ajax","crud","gii","template","database","modal","password-validation"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Denis Korolkov",
            "email": "right.ked@gmail.com",
            "homepage": "https://github.com/denkorolkov?tab=repositories"
        },
        {
            "name": "Rohit Singhal",
            "email": "errohitsinghal@gmail.com",
            "homepage": "https://github.com/errohitsinghal"
        }
    ],
    "support": {
        "issues": "https://github.com/errohitsinghal/yii2-ajaxcrud-bs5-advanced/issues?state=open",
        "source": "https://github.com/errohitsinghal/yii2-ajaxcrud-bs5-advanced"
    },
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-bootstrap5": "*",
	    "kartik-v/yii2-grid": "^3.0.4",
        "kartik-v/yii2-mpdf": "^1.0.0",
        "kartik-v/yii2-editable": "^1.7.3",
        "kartik-v/yii2-detail-view": "@dev"
    },
    "autoload": {
        "psr-4": {
            "denkorolkov\\ajaxcrud\\": "src/"
        }
    },
    "extra": {
        "bootstrap": "denkorolkov\\ajaxcrud\\Bootstrap",
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
